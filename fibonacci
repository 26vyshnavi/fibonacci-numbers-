def fibonacci(n):
    if(n<2):
        return 1
    return(fibonacci(n-1)+fibonacci(n-2))
n = int(input("enter the no of terms : "))
for i in range(n):
    print("fibonacci(",i,"=)",fibonacci(i))
    
    output:- .
enter the no of terms : 6
fibonacci( 0 =) 1
fibonacci( 1 =) 1
fibonacci( 2 =) 2
fibonacci( 3 =) 3
fibonacci( 4 =) 5
fibonacci( 5 =) 8
